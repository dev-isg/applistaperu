<?xml version="1.0" encoding="UTF-8" ?>

<widget xmlns = "http://www.w3.org/ns/widgets"
    xmlns:gap = "http://phonegap.com/ns/1.0"
    id        = "com.novasoftware.annualtargets"
    version   = "1.0.1">
  <name>ListaPerú</name>

  <description>
    Listado de todos los lugares que necesites ir. agentes, emergencia, ocio y más.
  </description>

  <author href="http://innovationssystems.com" email="informes@innovationssystems.com">
      innovations systems group
  </author>

  <gap:platform name="android" />

  <preference name="permissions"      value="none"/>
  <preference name="phonegap-version" value="2.9.0" />
  <preference name="orientation"      value="portrait" />
  <preference name="target-device"    value="universal" />
  <preference name="fullscreen"       value="false" />

  <icon src="style/images/icon/android/drawable-hdpi/ic_launcher.png" />
  <icon src="style/images/icon/android/drawable-ldpi/ic_launcher.png" />
  <icon src="style/images/icon/android/drawable-mdpi/ic_launcher.png" />
  <icon src="style/images/icon/android/drawable-xhdpi/ic_launcher.png" />
  <icon src="style/images/icon/android/drawable-xxhdpi/ic_launcher.png" />

  <gap:splash src="style/images/icon/android/drawable-xxhdpi/ic_launcher.png" />

  <plugin name="InAppBrowser" value="CDVInAppBrowser" />
  <access origin="http://192.168.1.50" subdomains="true" />
</widget>
