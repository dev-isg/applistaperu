<?xml version="1.0" encoding="UTF-8" ?>

<widget xmlns = "http://www.w3.org/ns/widgets"
    xmlns:gap = "http://phonegap.com/ns/1.0"
    id        = "com.novasoftware.annualtargets"
    version   = "1.0.1">
<access uri="http://192.168.1.50:8082" subdomains="true" />
  <name>ListaPerú</name>

  <description>
    Listado de todos los lugares que necesites ir. agentes, emergencia, ocio y más.
  </description>

  <author href="http://innovationssystems.com" email="informes@innovationssystems.com">
      innovations systems group
  </author>

  <gap:platform name="android" />

  <feature name="http://api.phonegap.com/1.0/battery"/>
  <feature name="http://api.phonegap.com/1.0/file"/>
  <feature name="http://api.phonegap.com/1.0/media"/>
  <feature name="http://api.phonegap.com/1.0/network"/>

  <preference name="permissions"      value="none"/>
  <preference name="phonegap-version" value="2.9.0" />
  <preference name="orientation"      value="portrait" />
  <preference name="target-device"    value="universal" />
  <preference name="fullscreen"       value="false" />

  <icon src="icon.png" />
  <gap:splash src="splash.png" />

  <plugin name="InAppBrowser" value="CDVInAppBrowser" />
  <access origin="*" />
</widget>
